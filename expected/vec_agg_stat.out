-- numeric stats
SELECT vec_agg_count(nums) AS counts
	, vec_agg_sum(nums) AS sums
	, vec_trim_scale(vec_agg_mean(nums)) AS means
	, vec_agg_min(nums) AS mins
	, vec_agg_max(nums) AS maxes
FROM measurements;
